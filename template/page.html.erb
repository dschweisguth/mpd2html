<div class="heading"><h1>JOHNSON SHEET MUSIC COLLECTION</h1></div>

<table>
  <tr>
    <th>Accession #</th>
    <th class="row-title"><%= column_header MPD2HTML::Page::Title %></th>
    <th class="row-name"><%= column_header MPD2HTML::Page::Composers %></th>
    <th class="row-name"><%= column_header MPD2HTML::Page::Lyricists %></th>
    <th>Source type</th>
    <th class="row-title">Source</th>
    <th>Date</th>
    <th>Location</th>
  </tr>
  <% items.each do |item| %>
    <tr>
      <td><%= item.accession_number %></td>
      <td><%= item.title %></td>
      <td><%= with_breaks item.composers %></td>
      <td><%= with_breaks item.lyricists %></td>
      <td><%= with_breaks item.source_types %></td>
      <td><%= with_breaks item.source_names %></td>
      <td><%= with_breaks item.dates %></td>
      <td><%= item.location %></td>
    </tr>
  <% end %>
</table>
